<%- include ('header'); -%>
    <section id="customer">

        <div class="customerTable table-responsive-sm">
            <table class="table table-striped  table-hover text-center ">
                <thead>
                    <tr>
                        
                        <th>Name</th>
                        <th>Email</th>
                        <th>Current Balance</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <% newData.forEach(function(item){ %>
                            <td><%= item.name %> </td>
                            <td><%= item.emailId %></td>
                            <td><%= item.currentbalance %></td>
                            <td><a href="/customer/<%=item.name%>"><button class="btn btn-danger" type="button">Details</button></a></td>
                            </tr>
                        <%});%>


                </tbody>
            </table>
        </div>
    </section>


    <%- include ('footer'); -%>